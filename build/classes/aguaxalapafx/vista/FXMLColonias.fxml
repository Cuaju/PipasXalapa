<AnchorPane id="AnchorPane" prefHeight="507.0" prefWidth="1133.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aguaxalapafx.controlador.FXMLColoniasController">
